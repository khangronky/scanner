    "ocr:run": "cd ocr && .\\venv\\Scripts\\modal run modal_deployment.py",
    "ocr:serve": "cd ocr && .\\venv\\Scripts\\modal serve modal_deployment.py",
    "ocr:deploy": "cd ocr && .\\venv\\Scripts\\modal deploy modal_deployment.py",
    "test": "vitest"
  },
